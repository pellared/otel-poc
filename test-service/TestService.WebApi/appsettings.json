{
   "serilog": {
      "minimumLevel": {
         "using": [ "Serilog.Sinks.Console" ],
         "default": "Information",
         "override": {
            "Microsoft": "Error",
            "system": "Error"
         }
      },
      "writeTo": [
         {
            "name": "Console",
            "args": {
               "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
            }
         }
      ],
      "enrich": [ "FromLogContext", "WithMachineName", "WithAssemblyName", "WithAssemblyVersion", "WithCorrelationId" ]
   }
}
